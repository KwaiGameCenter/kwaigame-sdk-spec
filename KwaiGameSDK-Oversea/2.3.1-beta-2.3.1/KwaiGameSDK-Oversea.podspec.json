{
  "name": "KwaiGameSDK-Oversea",
  "platforms": {
    "ios": "10.0"
  },
  "version": "2.3.1-beta-2.3.1",
  "license": "MIT",
  "description": "组件: KwaiGameSDK-Oversea\n快手游戏融合SDK",
  "summary": "快手游戏融合SDK",
  "homepage": "https://sdkdocs.game.kuaishou.com/#/",
  "source": {
    "http": "https://static.yximgs.com/udata/pkg/GameSource-CDN/KwaiGameSDK_KwaiGameSDK-Oversea_2.3.1-beta-2.3.1_b75f.zip",
    "type": "zip"
  },
  "authors": {
    "gamecenter": "gamecenter@kuaishou.com"
  },
  "default_subspecs": [
    "Core"
  ],
  "dependencies": {
    "KwaiGameSDK/Core": [
      "= 2.3.1-beta-2.3.1"
    ],
    "MainFrameworks_dependency/Core": [
      "= 2.3.1-beta-2.3.1"
    ]
  },
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": [
      "$(inherited)",
      "/usr/lib/swift",
      "$(TOOLCHAIN_DIR)/usr/lib/swift/$(PLATFORM_NAME)",
      "$(TOOLCHAIN_DIR)/usr/lib/swift-5.0/$(PLATFORM_NAME)"
    ],
    "LD_RUNPATH_SEARCH_PATHS": [
      "$(inherited)",
      "'/usr/lib/swift'"
    ],
    "ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES": "YES"
  },
  "subspecs": [
    {
      "name": "Core",
      "platforms": {
        "ios": "10.0"
      },
      "source_files": "headers/*.{h,hpp}",
      "public_header_files": "headers/*.{h,hpp}",
      "vendored_libraries": null,
      "vendored_frameworks": "frameworks/*.{framework}",
      "resources": "resources/*.*",
      "libraries": [
        "c++",
        "sqlite3",
        "stdc++",
        "z"
      ],
      "frameworks": [
        "Accelerate",
        "AdSupport",
        "AVFoundation",
        "CFNetwork",
        "CoreFoundation",
        "CoreGraphics",
        "CoreTelephony",
        "CoreText",
        "Foundation",
        "ImageIO",
        "LocalAuthentication",
        "MessageUI",
        "MobileCoreServices",
        "OpenGLES",
        "QuartzCore",
        "QuickLook",
        "SafariServices",
        "Security",
        "SystemConfiguration",
        "UIKit",
        "WebKit"
      ]
    }
  ]
}
